{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.rsdouglas/janee",
  "description": "Secrets management for AI agents via MCP. Janee acts as a secure proxy that manages API keys and credentials so AI agents never see raw secrets â€” they get capability-scoped, auditable access instead.",
  "repository": {
    "url": "https://github.com/rsdouglas/janee",
    "source": "github"
  },
  "version": "0.8.0",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@true-and-useful/janee",
      "version": "0.8.0",
      "transport": {
        "type": "stdio"
      },
      "runtime": "node",
      "environmentVariables": [
        {
          "name": "JANEE_CONFIG",
          "description": "Path to Janee configuration file",
          "isRequired": false,
          "format": "string",
          "isSecret": false
        }
      ]
    }
  ]
}
